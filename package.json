{
	"name": "stewed",
	"version": "0.17.0",
	"description": "Stewed Framework in CSS",
	"license": "MIT",
	"homepage": "https://github.com/maurovieirareis/stewed",
	"author": "Mauro Reis Vieira <maurovieirareis@gmail.com>",
	"repository": "https://github.com/maurovieirareis/stewed",
	"bin": {
		"stewed": "./src/cli.js"
	},
	"scripts": {
		"start": "http-server",
		"watch": "./node_modules/.bin/node-sass -w src/packages/stewed.scss dist/stewed.css",
		"build": "./node_modules/.bin/node-sass src/packages/stewed.scss dist/stewed.min.css --output-style compressed",
		"build:demos": "webpack --config ./demos/webpack.config.js --mode production",
		"lint": "stylelint \"src/**/*.scss\" && eslint \"src/**/*.js\" \"src/**/**/*.js\"",
		"format": "yarn run lint && prettier-stylelint --write \"src/**/*.+(css|scss)\" && prettier --write \"src/**/*.+(js|md|ts)\"",
		"test": "echo \"Warning: no test specified\" && exit 0"
	},
	"devDependencies": {
		"babel-preset-env": "^1.7.0",
		"babel-register": "^6.26.0",
		"css-loader": "^2.0.0",
		"eslint": "4.15.0",
		"eslint-config-prettier": "^4.1.0",
		"eslint-plugin-prettier": "^3.0.1",
		"extract-text-webpack-plugin": "^4.0.0-beta.0",
		"file-loader": "^0.11.1",
		"ignore-styles": "^5.0.1",
		"jsdom": "^11.10.0",
		"jsdom-global": "^3.0.2",
		"mocha": "^5.2.0",
		"node-sass": "^4.5.2",
		"prettier": "^1.16.4",
		"prettier-stylelint": "^0.4.2",
		"prismjs": "^1.15.0",
		"remarkable": "^1.7.1",
		"sass-loader": "^7.0.1",
		"script-loader": "^0.7.0",
		"source-map-loader": "^0.2.3",
		"style-loader": "^0.20.3",
		"stylelint": "^7.8.0",
		"stylelint-config-standard": "^16.0.0",
		"stylelint-order": "^0.5.0",
		"stylelint-scss": "^1.4.1",
		"stylelint-selector-bem-pattern": "^2.0.0",
		"vue": "^2.5.20",
		"vue-loader": "15.4.2",
		"vue-router": "^3.0.2",
		"vue-style-loader": "^4.1.2",
		"vue-template-compiler": "^2.5.20",
		"vuex": "^3.0.1",
		"webpack": "^4.27.1",
		"webpack-cli": "^3.1.2",
		"webpack-dev-server": "^3.2.1"
	},
	"dependencies": {
		"autoprefixer": "^9.4.5",
		"babel-eslint": "^10.0.1",
		"babel-plugin-transform-runtime": "^6.23.0",
		"chalk": "^2.4.1",
		"figlet": "^1.2.1",
		"fs-extra": "^7.0.1",
		"inquirer": "^6.2.2",
		"lodash": "^4.17.11",
		"node-emoji": "^1.8.1",
		"pretty-hrtime": "^1.0.3",
		"shelljs": "^0.8.3"
	}
}
